    public NovaAvaliacao() {
        try {
            modeloMaturidade = new ModeloComboMaturidade(new MaturidadeFacade().retornaTodasMaturidade());
        } catch (Exception ex) {
            Logger.getLogger(NovaAvaliacao.class.getName()).log(Level.SEVERE, null, ex);
        }
        modeloAvaliador = new DefaultListModel();
        modeloProjeto = new DefaultListModel();
        initComponents();
        maturidade.setModel(modeloMaturidade);
        projetos.addItemListener(new ItemListener() {

            public void itemStateChanged(ItemEvent e) {
                JRadioButton rb = (JRadioButton) e.getSource();
                boolean habilita = rb.isSelected();
                idProjeto.setEnabled(habilita);
                nomeProjeto.setEnabled(habilita);
                listaProjetos.setEnabled(habilita);
                incluirProjeto.setEnabled(habilita);
                excluirProjeto.setEnabled(habilita);
                procuraProjeto.setEnabled(habilita);
            }
        });
        projetos.setSelected(true);
        setVisible(true);
        setSize(700, 550);
        /*try {
        listaMaturidade = new MaturidadeFacade().retornaTodasMaturidade();

        for (Maturidade matu : listaMaturidade) {
